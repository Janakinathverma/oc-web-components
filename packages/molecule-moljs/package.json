{
  "name": "@openchemistry/molecule-moljs",
  "version": "0.4.12",
  "description": "Stencil Wrapper for 3Dmoljs with support for cjson chemical data",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "@openchemistry/moljs-es": "^0.1.6",
    "@openchemistry/types": "^0.8.2",
    "@openchemistry/utils": "^0.6.2",
    "@types/lodash-es": "^4.17.1",
    "lodash-es": "^4.17.10",
    "resize-observer-polyfill": "^1.5.0"
  },
  "devDependencies": {
    "@stencil/core": "^0.13.2",
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenChemistry/oc-web-components"
  },
  "author": "Kitware, Inc.",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/OpenChemistry/oc-web-components"
  },
  "homepage": "https://github.com/OpenChemistry/oc-web-components",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
