{
  "name": "@openchemistry/molecule",
  "version": "0.4.0",
  "description": "Stencil component to display chemical json data",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "@openchemistry/molecule-menu": "^0.6.1",
    "@openchemistry/molecule-vtkjs": "^0.2.0",
    "@openchemistry/types": "^0.6.1",
    "@openchemistry/utils": "^0.4.1",
    "@openchemistry/vibrational-spectrum": "^0.1.11",
    "lodash-es": "^4.17.10",
    "memoize-one": "^4.0.2",
    "split-me": "^0.3.1"
  },
  "devDependencies": {
    "@stencil/core": "^0.12.4",
    "@types/jest": "^23.3.1",
    "@types/lodash-es": "^4.17.1",
    "jest": "^23.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenChemistry/oc-web-components"
  },
  "author": "Kitware, Inc.",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/OpenChemistry/oc-web-components"
  },
  "homepage": "https://github.com/OpenChemistry/oc-web-components",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
