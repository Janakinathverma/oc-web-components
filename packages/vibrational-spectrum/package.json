{
  "name": "@openchemistry/vibrational-spectrum",
  "version": "0.1.22",
  "description": "Component to plot the vibrational spectrum of in the cjson chemical data",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "@openchemistry/types": "^0.8.5",
    "d3": "^5.9.1",
    "lodash-es": "^4.17.11",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@stencil/core": "^0.18.0",
    "@types/d3": "^5.7.1",
    "@types/jest": "^24.0.9",
    "@types/lodash-es": "^4.17.2",
    "jest": "^24.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenChemistry/oc-web-components.git"
  },
  "author": "Kitware, Inc.",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/OpenChemistry/oc-web-components"
  },
  "homepage": "https://github.com/OpenChemistry/oc-web-components",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
